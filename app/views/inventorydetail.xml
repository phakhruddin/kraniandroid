<Alloy>
	<Model src="dummy" />
  <Window id="inventorydetail_window" title="inventory Detail"  backgroundColor="white">
	  <TableView id="inventorydetail_table"  backgroundColor="transparent">
	  	<TableViewSection headerTitle="inventoryDetails">
		  <TableViewRow id="inventorydetail_row" Title="inventoryDetail">
		  	  <Label id="inventoryName" text="{dummy.inventoryname}" />
		  </TableViewRow>
		  <TableViewRow id="clientinventorydetail_row" Title="clientinventoryDetail">
	   		  <Label id="fullname" text="{dummy.fullname}" />
	   	  </TableViewRow>
		  <TableViewRow id="phoneinventorydetail_row" Title="PhoneinventoryDetail">
	   		  <Label id="phone" text="{dummy.phone}" />
	   	  </TableViewRow>
	   	  <TableViewRow id="addressinventorydetail_row" Title="addressinventoryDetail">
	   		  <Label id="address" text="{dummy.address}" />
	   	  </TableViewRow>
	   	  <TableViewRow id="emailinventorydetail_row" Title="emailinventoryDetail">
	   		  <Label id="email" text="{dummy.email}" />
	   	  </TableViewRow>
	   	  <TableViewRow id="companyinventorydetail_row" Title="companyinventoryDetail">
	   		  <Label id="company" text="{dummy.company}" />
		  </TableViewRow>
	  </TableViewSection>
	  <TableViewSection headerTitle="Pending Invoice">
		  <TableViewRow id="invoiceinventorydetail_row" Title="invoiceinventoryDetail">
	   		  <Label id="invoice" text="{dummy.invoice}" />
		  </TableViewRow>
	  </TableViewSection>
	  <TableViewSection headerTitle="Percent Complete & Date Due">
		  <TableViewRow id="percentcompletedetail_row" Title="percentcompleteDetail">
	   		  <Label id="percentcomplete" text="{dummy.percentcomplete}" />
		  </TableViewRow>
		  <TableViewRow id="dateduedetail_row" Title="datedueDetail">
	   		  <Label id="datedue" text="{dummy.datedue}" />
		  </TableViewRow>
	  </TableViewSection>
	  <TableViewSection headerTitle="Next Appointment">
		  <TableViewRow id="nextapptdetail_row" Title="nextapptDetail">
	   		  <Label id="nextappt" text="{dummy.nextappt}" />
		  </TableViewRow>
	  </TableViewSection>
	  <TableViewSection headerTitle="Notes">
		  <TableViewRow id="notesdetail_row" Title="notesDetail">
	   		  <Label id="notes" text="{dummy.notes}" />
		  </TableViewRow>
	  </TableViewSection>
	  </TableView>
  </Window>
</Alloy>